{
  "listeners": {
    "*:8000": {
      "pass": "routes/proj.urls"
    }
  },
  "routes": {
    "proj.urls": [
      {
        "match": {
          "uri": "~^/extra/"
        },
        "action": {
          "pass": "routes/proj.extra_urls"
        }
      },
      {
        "action": {
          "pass": "applications/nereids"
        }
      }
    ],
    "proj.extra_urls": [
      {
        "match": {
          "uri": [
            "~^/extra/hi/$"
          ]
        },
        "action": {
          "pass": "applications/hesperides"
        }
      },
      {
        "action": {
          "pass": "applications/nereids"
        }
      }
    ]
  },
  "applications": {
    "nereids": {
      "type": "python 3",
      "protocol": "wsgi",
      "path": "/app",
      "module": "proj.wsgi",
      "callable": "application"
    },
    "hesperides": {
      "type": "python 3",
      "protocol": "asgi",
      "path": "/app",
      "module": "proj.asgi",
      "callable": "application"
    }
  }
}
