{
  "routes": {
    "proj.urls": [
      {
        "match": {
          "uri": "~^/extra/*"
        },
        "action": {
          "pass": "routes/proj.extra_urls"
        }
      },
      {
        "action": {
          "pass": "applications/sync"
        }
      }
    ],
    "proj.extra_urls": [
      {
        "match": {
          "uri": [
            "~^/extra/hi/$"
          ]
        },
        "action": {
          "pass": "applications/async"
        }
      },
      {
        "action": {
          "pass": "applications/sync"
        }
      }
    ]
  }
}
